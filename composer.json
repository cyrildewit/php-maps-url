{
    "name": "cyrildewit/php-maps-urls",
    "description": "Generate URLs for the Google Maps URLs API",
    "type": "library",
    "license": "Apache-2.0",
    "homepage": "https://github.com/cyrildewit/php-maps-urls",
    "keywords": [
        "php",
        "google",
        "maps",
        "url",
        "urls"
    ],
    "support": {
        "email": "opensource@cyrildewit.nl",
        "issues": "https://github.com/cyrildewit/php-maps-urls/issues",
        "source": "https://github.com/cyrildewit/php-maps-urls"
    },
    "authors": [
        {
            "name": "Cyril de Wit",
            "email": "info@cyrildewit.nl",
            "homepage": "https://cyrildewit.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "CyrildeWit\\MapsUrls\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CyrildeWit\\MapsUrls\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
